#!/usr/bin/env swipl
% vim: set filetype=prolog: ts=2 sw=2 sts=2 expandtab:cindent:formatoptions+=cro %

	
% singletons (unused var).
a(X,Y) :- print(Y).


% dropped end of clause mark

a :- b.
b :- c
d :- e.

% undefined operator

test(a and b) :- test(a), test(b).

% one letter typo.

a(X,Y,Z) :- has(X,Y,Z).

% same cluase incorrectly defined twice.

a(1).
a(2).

somethingElse.

a(3).

:- halt.
